        "Excel": "https://forms.gle/BV6gxxEkJJXq1z3A9",
        "PowerPoint": "https://forms.gle/BV6gxxEkJJXq1z3A9"
    },
    "viewSlidesUrls": {
        "Windows": "https://docs.google.com/presentation/d/1PnVhjiBHnruTwOeLXjjpcr-ytwy0bGzcMwr4JEJAnIw",
